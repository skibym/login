{
  "attr-bans": [
    "align",
    "background",
    "bgcolor",
    "border",
    "frameborder",
    "longdesc",
    "marginwidth",
    "marginheight",
    "scrolling"
  ],
  "attr-name-style": "lowercase",
  "attr-name-ignore-regex": "xlink:href|http-equiv",
  "attr-no-unsafe-char": false,
  "class-style": "bem",
  "doctype-first": true,
  "doctype-html5": true,
  "attr-no-dup": true,
  "attr-order": [
    "class",
    "id",
    "name",
    "src",
    "for",
    "type",
    "href",
    "value",
    "title",
    "alt",
    "role",
    "aria"
  ],
  "attr-quote-style": "double",
  "class-no-dup": true,
  "head-req-title": true,
  "html-req-lang": true,
  "img-req-alt": true,
  "indent-style": "spaces",
  "indent-width": 2,
  "indent-width-cont": true,
  "input-req-label": true,
  "label-req-for": true,
  "line-end-style": "crlf",
  "tag-name-match": true,
  "tag-self-close": "never",
  "spec-char-escape": false,
  "tag-bans": [
    "b",
    "i",
    "style"
  ],
  "title-max-len": false
}
